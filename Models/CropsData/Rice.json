        "Diseases": [
            {
                "disease_name": "Bacterial leaf blight",
                "images": {
                    "image1": "https://encrypted-tbn2.gstatic.com/images?q=tbn:ANd9GcR4yKSHqnjBviFjmWIweOJtAFsPXWw3PR_mukICdUuXgArbZk8-c7YxozPg6Gay"
                },
                "description": "A bacterial disease that causes pale yellow to brown lesions on the leaves of rice plants. The lesions can eventually enlarge and merge, causing the leaves to die."
            },
            {
                "disease_name": "Brown spot",
                "images": {
                    "image1":"https://encrypted-tbn2.gstatic.com/images?q=tbn:ANd9GcTfCs0fekNgvf9kCpnXMvYHsiZ-uLYtD6q9e37Mu9YiKwZOuiVCDQpSzp-beUwW"
                },
                "description": "A fungal disease that causes small, circular, brown spots on the leaves of rice plants. The spots can eventually enlarge and merge, causing the leaves to die."
            },
            {
                "disease_name": "False smut",
                "images": {
                    "image1":"https://encrypted-tbn3.gstatic.com/images?q=tbn:ANd9GcREMfFerMkmytnRm6O9wyJ5-QiWnR-xZ66nXNBcCP48FMmv8WbW7xHouwAZdoTJ"
                },
                "description": "A fungal disease that causes black, sooty masses to develop on the flowers of rice plants. The infected flowers are sterile and do not produce grain."
            },
            {
                "disease_name": "Rice blast",
                "images":  {
                    "image1" :"https://encrypted-tbn2.gstatic.com/images?q=tbn:ANd9GcT8uTJTxR2_T3iIVTiShUd42lfHOP5eZYAL9Kf4k0wrR1LKcktJytEzUPBcIDgE"
                },
                "description": "A fungal disease that causes oval, gray lesions on the leaves, stems, and flowers of rice plants. The lesions can eventually enlarge and merge, causing the plants to die."
            },
            {
                "disease_name": "Tungro",
                "images": {
                    "image1":"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQszfN-S35GYlYjim48i-yvqdWx7QOF8wgtkqS5jxBUADmq7KtuARGiTh7aaGas"
                },
                "description": "A viral disease that causes stunting, yellowing, and twisting of the leaves of rice plants. The infected plants may also produce distorted or empty grains."
            }
        ],
